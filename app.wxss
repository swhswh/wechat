/**app.wxss**/
@import 'weui.wxss';
.container{
  height: 100%;
  width: 100%;
  padding: 0;
  align-items: center;
}


page{
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page__hd {
    padding: 40px;
}
.page__bd {
    padding-bottom: 40px;
}
.page__bd_spacing {
    padding-left: 15px;
    padding-right: 15px;
}

.page__ft{
    padding-bottom: 10px;
    text-align: center;
}

.page__title {
    text-align: left;
    font-size: 14px;
    color: #555;
    font-weight: 400;
}

.page__desc {
    margin-top: 2px;
    color: #888888;
    text-align: left;
    font-size: 13px;
}